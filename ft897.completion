_ft897_completion() {
  local commandpath
  COMPREPLY=()
  commandpath="${PWD}/${BASH_SOURCE%/*}"
  commands=$(for d in ${commandpath}/{commands,scripts,bookmarks}; do cd ${d}; ls; done)
  COMPREPLY=($(compgen -W "${commands[*]}" -- "${COMP_WORDS[COMP_CWORD]}"))
}
complete -F _ft897_completion ft897
